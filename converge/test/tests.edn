{:tests    [{:kaocha.testable/type :kaocha.type/clojure.test
             :kaocha.testable/id   :unit-clj
             :kaocha/ns-patterns   ["-test$"]
             :kaocha/source-paths  ["src"]
             :kaocha/test-paths    ["test"]}
